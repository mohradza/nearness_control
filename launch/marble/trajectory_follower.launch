<launch>

    <!-- <arg name="enable_debug" /> -->
    <arg name="vehicle_name" default="T02"/>
    <node pkg="nearness_control" type="trajectory_follower" name="trajectory_follower_node" output="screen">
        <!-- TOPICS -->
        <remap from="trajectory_follower/odometry_map" to="odometry_map"/>
        <remap from="trajectory_follower/trajectory" to="pose_graph"/>
        <remap from="trajectory_follower/task" to="fake_task"/>
        <remap from="trajectory_follower/follow_traj" to="follow_traj_fake"/>

        <param name="vehicle_name" value="$(arg vehicle_name)"/>

    </node>

    <include file="$(find lcd_pkg)/launch/lcd.launch"/>

</launch>
