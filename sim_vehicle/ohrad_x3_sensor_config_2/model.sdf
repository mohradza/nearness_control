                    <samples>360</samples>
                    <resolution>0.1</resolution>
                    <min_angle>-3.1459</min_angle>
                    <max_angle>3.1459</max_angle>
                  </horizontal>
                  <vertical>
                    <samples>64</samples>
                    <resolution>1</resolution>
                    <min_angle>-1.5706</min_angle>
                    <max_angle>1.5706</max_angle>
                  </vertical>
                </scan>
                <range>
